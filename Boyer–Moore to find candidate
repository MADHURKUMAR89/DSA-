#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements:\n");
    scanf("%d", &n);

    int arr[100];

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }



    int count = 0;
    int candidate = 0;
    for (int i = 0; i < n; i++) {
        if (count == 0) {
            candidate = arr[i];
            count = 1;
        } else if (arr[i] == candidate) {
            count++;
        } else {
            count--;
        }
    }

  
    int countCheck = 0;
    for (int i = 0; i < n; i++) {
        if (arr[i] == candidate) {
            countCheck++;
        }
    }

    if (countCheck > n / 2) {
        printf("Majority element is %d\n", candidate);
    } else {
        printf("No majority element found\n");
    }

    return 0;
}
