
#include <stdio.h>

int main() {
int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n]; // Declare array of size n

    printf("Enter %d integers:\n", n);
    for(int i = 0; i < n; i++) {
        // Use & to point to the memory address of each index
        scanf("%d", &arr[i]);}
       int hash_arr [12];
        int m ;
        for(int i = 0; i < n ; i++)
        {  m = arr[i] ;
        hash_arr[m]++;
        }
for(int i = 0; i < 13; i++) {
        printf("%d ", hash_arr[i]);
    }

    return 0;
}
