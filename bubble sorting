#include <stdio.h>

int main() {
    int n;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
for (int m =  0 ; m < n-1 ; m++){
    for (int i = 0 ; i < n-1-m ; i++){
        if ( arr[i]>arr[i+1]){
            int temp = arr[i+1];
            arr[i+1] = arr[i];
            arr[i] = temp;
            }
    }}
     for (int i = 0; i < n; i++) {
        printf("%d", arr[i]);
    }

    

    return 0;
}
