#include <stdio.h>

int main() {
    int size;

    printf("Enter number of elements:\n");
    scanf("%d", &size);

    int arr[size];   

    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    int max = 0;
    int cnt = 0;

    for (int i = 0; i < size; i++) {
        if (arr[i] == 1) {
            cnt++;
        } else {
            cnt = 0;
        }

        if (cnt > max) {
            max = cnt;
        }
    }

    printf("Maximum consecutive 1s = %d\n", max);

    return 0;
}
