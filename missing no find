#include <stdio.h>

int main() {
    int arr[5];
    printf("Enter 5 integers:\n");

    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    int miss;
    int sum = 5 * (5 + 1) / 2;   // sum of 1 to 5
    int arrsum = 0;

    for (int i = 0; i < 5; i++) {
        arrsum += arr[i];
    }

    miss = sum - arrsum;
    printf("Missing number = %d", miss);

    return 0;
}
