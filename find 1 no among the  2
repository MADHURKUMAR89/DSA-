#include <stdio.h>

int main() {
    int n;

    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter array elements:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int result = 0;

    for (int i = 0; i < n; i++) {
        result ^= arr[i];
    }

    printf("Single number is: %d\n", result);

    return 0;
}
