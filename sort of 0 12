// Online C compiler to run C program online
#include <stdio.h>

#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements:\n");
    scanf("%d", &n);

    int arr[100];  // array declaration

    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

 


    int mid = 0;
    int low = 0;
    int high = n-1;
    for (int i = 0 ; i < n ; i++){
        if ( arr[mid]==0 ){
            int temp = arr[low];
            arr[low] = arr[mid];
            arr[mid] = temp;
            mid++;
            low++;
            
        }
        else if (arr[mid]==1){
            
        mid++;
        
        }
        else{
              int temp = arr[high];
            arr[high] = arr[mid];
            arr[mid] =temp;
          high--;
        }
    }

    for(int i = 0; i < n; i++) {
        printf("%d", arr[i]);
    }

    return 0;
}
