#include <stdio.h>

int main() {
    int n, i;
    int arr[100];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int g = arr[0];   

    for (i = 1; i < n; i++) {
        if (arr[i] > g) {
            g = arr[i];
        }
    }

    printf("Greatest element = %d\n", g);

    return 0;
}
